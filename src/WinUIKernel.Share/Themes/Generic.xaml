<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:Richasy.WinUIKernel.Share.Converters">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ms-appx:///Richasy.WinUIKernel.Share/Base/AppTitleBar/AppTitleBar.xaml" />
        <ResourceDictionary Source="ms-appx:///Richasy.WinUIKernel.Share/Base/ImageExBase/ImageExBase.xaml" />
        <ResourceDictionary Source="ms-appx:///Richasy.WinUIKernel.Share/Base/CardControl/CardControl.xaml" />
        <ResourceDictionary Source="ms-appx:///Richasy.WinUIKernel.Share/Base/DraggableCardControl/DraggableCardControl.xaml" />
        <ResourceDictionary Source="ms-appx:///Richasy.WinUIKernel.Share/Base/SettingsControls/SettingsCard/SettingsCard.xaml" />
        <ResourceDictionary Source="ms-appx:///Richasy.WinUIKernel.Share/Base/SettingsControls/SettingsExpander/SettingsExpander.xaml" />
        <ResourceDictionary Source="ms-appx:///Richasy.WinUIKernel.Share/Base/AppToggleCard/AppToggleCard.xaml" />
        <ResourceDictionary Source="ms-appx:///Richasy.WinUIKernel.Share/Base/CustomNavigationViewResources.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityReverseConverter" IsReverse="True" />
    <converters:ObjectToBoolConverter x:Key="ObjectToBoolConverter" />
    <converters:ObjectToBoolConverter x:Key="ObjectToBoolReverseConverter" IsReverse="True" />
    <converters:ObjectToVisibilityConverter x:Key="ObjectToVisibilityConverter" />
    <converters:ObjectToVisibilityConverter x:Key="ObjectToVisibilityReverseConverter" IsReverse="True" />
    <converters:BoolToIconVariantConverter x:Key="BoolToIconVariantConverter" />
</ResourceDictionary>
