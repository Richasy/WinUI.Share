  	</Page>
  </ItemGroup>

  <ItemGroup>
    <Page Update="Chat\AzureOpenAIChatSettingControl.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
  </ItemGroup>

  <ItemGroup>
    <Page Update="Audio\OpenAIAudioSettingControl.xaml">
      <Generator>MSBuild:Compile</Generator>
    </Page>
  </ItemGroup>
</Project>